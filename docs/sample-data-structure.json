      "required_fields": ["current_price", "currency"],
      "optional_fields": ["list_price", "buy_box_price", "discount_percent"],
      "validation_rules": {
        "price_range": {"min": 0, "max": 10000},
        "currency": "^[A-Z]{3}$",
        "discount_percent": {"min": 0, "max": 100}
      }
